<UserControl x:Class="AgendaMVC_WPF.view.Contactes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AgendaMVC_WPF.view"
        mc:Ignorable="d"
        d:DesignHeight="450" d:DesignWidth="800"
        >
    <Grid>
        <ListView x:Name="LV_Artists" HorizontalAlignment="Left" Height="400" Margin="10,10,0,0" VerticalAlignment="Top" Width="780">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="nom" DisplayMemberBinding="{Binding Name}"/>
                    <GridViewColumn Header="telephone" DisplayMemberBinding="{Binding Telephone}"/>
                    <GridViewColumn Header="contact" DisplayMemberBinding="{Binding Adresse}"/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</UserControl>
